{
  "name": "@cablanchard/matrix",
  "version": "0.0.2",
  "description": "Containers for some kind of number",
  "repository": {
    "type": "git",
    "url": "git@github.com:cblanc/matrix"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test node_modules/.bin/nyc node_modules/.bin/mocha",
    "lint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json lib/**/*.ts",
    "build": "npm run lint && node_modules/.bin/tsc",
    "coverage": "node_modules/.bin/nyc report --reporter=text-lcov | node_modules/.bin/coveralls || true",
    "prepare": "npm run build"
  },
  "mocha": {
    "spec": "tests/*.ts",
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "engines": {
    "node": ">=8.0"
  },
  "author": "cblanc",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "~4.2.0",
    "@types/mocha": "~5.2.5",
    "@types/node": "~13.1.0",
    "chai": "~4.2.0",
    "coveralls": "~3.0.2",
    "mocha": "~7.0.0",
    "nyc": "~15.0.0",
    "source-map-support": "~0.5.10",
    "ts-node": "~8.6.1",
    "tslint": "~5.20.0",
    "typescript": "~3.7.2",
    "typings": "~2.1.1"
  },
  "dependencies": {}
}
